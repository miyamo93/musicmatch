.wrappar
  .main-header
  - if user_signed_in?
    .main-headers__signup
      = link_to "Mypage", class: 'get'
    .main-headers__login
      = link_to "Logout", destroy_user_session_path, method: :delete
  - else
    .main-headers__signup
      = link_to "Signup", new_user_registration_path, class: 'post'
    .main-headers__login
      = link_to "Login", new_user_session_path, class: 'post'